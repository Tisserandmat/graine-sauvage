{% extends 'base.html.twig' %}

{% block title %}Land{% endblock %}

{% block body %}
    <h1>Land</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ land.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ land.name }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ land.description }}</td>
            </tr>
            <tr>
                <th>City</th>
                <td>{{ land.city }}</td>
            </tr>
            <tr>
                <th>Address</th>
                <td>{{ land.address }}</td>
            </tr>
            <tr>
                <th>ZipCode</th>
                <td>{{ land.zipCode }}</td>
            </tr>
            <tr>
                <th>Price</th>
                <td>{{ land.price }}</td>
            </tr>
            <tr>
                <th>Slug</th>
                <td>{{ land.slug }}</td>
            </tr>
            <tr>
                <th>ImageName</th>
                <td>{{ land.imageName }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ land.updatedAt ? land.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('land_index') }}">back to list</a>

    <a href="{{ path('land_edit', {'id': land.id}) }}">edit</a>

    {{ include('land/_delete_form.html.twig') }}
{% endblock %}
